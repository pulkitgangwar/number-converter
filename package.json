{
  "name": "number-converter",
  "displayName": "number-converter",
  "description": "converts number into binary,decimal,octal and hexadecimal form",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:number-converter.convertBinaryToDecimal",
    "onCommand:number-converter.convertDecimalToBinary",
    "onCommand:number-converter.convertDecimalToHexa",
    "onCommand:number-converter.convertHexaToDecimal",
    "onCommand:number-converter.convertDecimalToOctal",
    "onCommand:number-converter.convertOctalToDecimal"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "number-converter.convertBinaryToDecimal",
        "title": "Convert Binary To Decimal"
      },
      {
        "command": "number-converter.convertDecimalToBinary",
        "title": "Convert Decimal To Binary"
      },
      {
        "command": "number-converter.convertDecimalToHexa",
        "title": "Convert Decimal To HexaDecimal"
      },
      {
        "command": "number-converter.convertHexaToDecimal",
        "title": "Convert HexaDecimal To Decimal"
      },
      {
        "command": "number-converter.convertDecimalToOctal",
        "title": "Convert Decimal To Octal"
      },
      {
        "command": "number-converter.convertOctalToDecimal",
        "title": "Convert Octal To Decimal"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
